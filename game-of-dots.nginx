server {
    listen 80;
    listen [::]:80;

    server_name gameofdots.prajaktarodrigues.website;

    location / {
        proxy_pass http://localhost:3222;
    }

    location /socket {
        proxy_pass http://localhost:3222;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
    }
}